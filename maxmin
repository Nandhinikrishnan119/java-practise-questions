import java.util.Scanner;
public class Main{
    public static void main(String[] args) {
         Scanner kbd=new Scanner(System.in);
         int n=kbd.nextInt();
         int a=kbd.nextInt(); 
         if(n>a){
         System.out.println("max n"+n);
         System.out.println("min a"+a);
         } else{
         System.out.println("max n"+a);
         System.out.println("min a"+n);

         }
         }
    }
Output:
4 2
max 4
min 2
