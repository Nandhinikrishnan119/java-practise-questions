    
import java.util.Scanner;
public class factno{
    public static void main(String[] args) {
        Scanner kbd=new Scanner(System.in);
        int n=kbd.nextInt();
        int fact=1;
        for (int i = 1; i <= n; i++) {
            fact=fact*i;
            System.out.println(fact);
            
        }
            
        }
    }
    


