  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n = kbd.nextInt();
	    int count=0;
        while(n>0)
        {
            n=n/10;
            count++;
            
        }
        System.out.print(count);
	}
}
