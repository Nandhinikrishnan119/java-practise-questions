import java.util.Scanner;
public class evenno {
    public static void main(String[] args) {
        Scanner kbd=new Scanner(System.in);
        int a=kbd.nextInt();
        int b=kbd.nextInt();
        for(int i=a;i<=b;i++){
            if(i%2==0){
            System.out.println(i);
            }
        }
    }}
    

