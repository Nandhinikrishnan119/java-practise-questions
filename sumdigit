  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n = kbd.nextInt();
	    int count=0;
	    int sum=0;
        while(n>0)
        {
            int dig=n%10;
            sum+=dig;
            n=n/10;
        }
        
        System.out.print(sum);
	}
}
